doctype html
html
    head
        title Abode api
        meta(name='viewport', content='width=device-width, initial-scale=1.0')
        link(href='https://fonts.googleapis.com/css2?family=Roboto:wght@300;400&display=swap')
        style
            include change-password.css
        script(type="text/javascript", src='https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js')
        script
            include change-password.js

    body
        block content
            div(class="logo")
                img(src="data:image/png;base64,")
            div(class="title") Change Password
            form(class="form" id="form" action="" method="post")
                div(class="item")
                    label(for="emailId") Email ID
                    input(type="text" id="emailId" placeholder="Enter your email address" required)
                    div(class="err" id="email_id_err")

                div(class="item")
                    label(for="password") Password
                    input(type="password" id="password" placeholder="Enter new password" required)
                    div(class="err" id="password_err")

                div(class="item")
                    label(for="verifyPassword") Re-Enter Password
                    input(type="password" id="verifyPassword" placeholder="Verify new password" required)
                    div(class="err" id="verify_password_err")

                div(class="item")
                    input(type="submit" value="Save" class="button")


            div(class="result_message success_message" id="success_msg")
                div(class="icon") &check;
                div(class="message") Password changed successfully.

            div(class="result_message error_message" id="err_msg")
                div(class="icon") &#9888;
                div(class="message") Oops something went wrong!
